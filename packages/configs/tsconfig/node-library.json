{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node library",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": ["es2015"],
    "module": "ESNext",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "target": "ESNext",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "strict": true
  }
}
