		"@formkit/auto-animate": "1.0.0-beta.6",
		"@iconify/svelte": "^3.1.3",
		"@playwright/test": "^1.28.1",
		"@resvg/resvg-js": "^2.1.0",
		"@skeletonlabs/skeleton": "^2.3.0",
		"@skeletonlabs/tw-plugin": "^0.2.2",
		"@storybook/addon-essentials": "^7.0.18",
		"@storybook/addon-interactions": "^7.0.18",
		"@storybook/addon-links": "^7.0.18",
		"@storybook/addon-svelte-csf": "^3.0.2",
		"@storybook/addon-viewport": "^7.0.11",
		"@storybook/blocks": "^7.0.18",
		"@storybook/svelte": "^7.0.18",
		"@storybook/sveltekit": "^7.0.18",
		"@storybook/testing-library": "^0.1.0",
		"@sveltejs/adapter-vercel": "3.0.1",
		"@sveltejs/kit": "^1.19.0",
		"@tailwindcss/forms": "^0.5.3",
		"@tailwindcss/typography": "^0.5.9",
		"@typescript-eslint/eslint-plugin": "^5.59.7",
		"@typescript-eslint/parser": "^5.59.7",
		"arktype": "1.0.14-alpha",
		"autoprefixer": "^10.4.14",
		"axios": "^1.3.4",
		"concurrently": "^8.0.1",
		"eslint": "^8.41.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-storybook": "^0.6.12",
		"eslint-plugin-svelte": "^2.29.0",
		"highlight.js": "^11.8.0",
		"highlight.run": "^7.4.0",
		"jose": "^4.15.3",
		"jwt-decode": "^3.1.2",
