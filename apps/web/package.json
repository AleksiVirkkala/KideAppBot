		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"satori": "^0.9.1",
		"satori-html": "^0.3.2",
		"storybook": "^7.0.18",
		"svelte": "^3.59.1",
		"svelte-check": "^3.4.3",
		"svelte-og": "workspace:*",
		"svelte-preprocess": "^5.0.4",
		"tailwind-merge": "^1.12.0",
		"tailwindcss": "^3.3.2",
